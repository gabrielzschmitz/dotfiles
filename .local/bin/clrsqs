#!/bin/bash
#         .             .              .		  
#         |             |              |           .	  
# ,-. ,-. |-. ,-. . ,-. |  ,_, ,-. ,-. |-. ,-,-. . |- ,_, 
# | | ,-| | | |   | |-' |   /  `-. |   | | | | | | |   /  
# `-| `-^ ^-' '   ' `-' `' '"' `-' `-' ' ' ' ' ' ' `' '"' 
#  ,|							  
#  `'							  
# GITHUB:https://github.com/gabrielzschmitz		  
# INSTAGRAM:https://www.instagram.com/gabrielz.schmitz/   
# DOTFILES:https://github.com/gabrielzschmitz/dotfiles/   

# Colored squares script edited by gabrielzschmitz
# and made by lr-tech(https://github.com/lr-tech/dotfiles)

s=$'\e[3'
b=$'\e[9'
m='m'
c=$'\e[0m'

for i in {0..7}; do
  echo -n "$b$i$m████$s$i$m▄$c"
done
echo

for i in {0..7}; do
  echo -n "$b$i$m████$s$i$m█$c"
done
echo

for i in {0..7}; do
  echo -n "$s$i$m ▀▀▀▀$c"
done

