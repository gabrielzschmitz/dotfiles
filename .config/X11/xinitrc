# DOTFILES:https://github.com/gabrielzschmitz/dotfiles/   
#!/bin/sh
kblayout &
/usr/lib/polkit-gnome/polkit-gnome-authentication-agent-1 &
keyrate &
xbanish -i mod4 -i mod1 -i shift -i control &
xrdb $HOME/.config/X11/xresources &
mpd &
dunst &
pulseaudio &
picom --experimental-backends -b &
udiskie --file-manager=pcmanfm --terminal=st --notify --no-tray --automount &
defaultdisplay &
defaultwallpaper &
disautolock &
while xsetroot -name " [`date +%a`] `date +%D` `date +%H:%M%p` "
do
	sleep 1
done &
exec dwm

